{
  "name": "disqus-proxy",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "kcors": "2",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.2.0",
    "log4js": "^1.1.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "socks5-https-client": "^1.2.0"
  },
  "devDependencies": {
    "classnames": "^2.2.5",
    "node-sass-chokidar": "^0.0.2",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build && npm run build:rename",
    "build:rename": "mv ./build/static/js/*.js ./build/static/js/disqus-proxy.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "license": "MIT"
}
